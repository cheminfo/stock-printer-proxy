        "test-only": "jest"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/superagent": "^4.1.15",
        "env-cmd": "^10.1.0",
        "eslint": "^8.17.0",
        "eslint-config-cheminfo-typescript": "^11.0.1",
        "fastify-tsconfig": "^1.0.1",
        "jest": "^28.1.1",
        "nodemon": "^2.0.16",
        "prettier": "^2.7.1",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.3"
    },
    "volta": {
        "node": "16.15.1"
    }
