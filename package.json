{
  "private": true,
  "name": "stock-printer-proxy",
  "version": "4.0.8",
  "description": "A public server to proxy requests to printers on the intranet",
  "main": "src/server.js",
  "repository": "git@github.com:cheminfo/stock-printer-proxy.git",
  "author": "Daniel Kostro <kostro.d@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@sinclair/typebox": "^0.20.5",
    "body-parser": "^1.19.0",
    "fastify": "^3.22.1",
    "fastify-cors": "^6.0.2",
    "rest-on-couch-client": "^3.3.1",
    "superagent": "^6.1.0"
  },
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "eslint": "eslint --cache src",
    "eslint-fix": "eslint --fix src",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "start": "env-cmd node lib/server.js",
    "start-dev": "env-cmd nodemon --config nodemon.json src/server.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.3",
    "@types/superagent": "^4.1.13",
    "env-cmd": "^10.1.0",
    "eslint": "^8.0.1",
    "eslint-config-cheminfo-typescript": "^10.1.1",
    "fastify-tsconfig": "^1.0.1",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "typescript": "^4.4.4"
  }
}
