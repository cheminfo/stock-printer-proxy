{
    "private": true,
    "name": "stock-printer-proxy",
    "version": "6.1.2",
    "description": "A public server to proxy requests to printers on the intranet",
    "main": "src/server.js",
    "repository": "git@github.com:cheminfo/stock-printer-proxy.git",
    "author": "Daniel Kostro <kostro.d@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@fastify/cors": "^8.2.0",
        "@fastify/swagger": "^8.3.1",
        "@fastify/swagger-ui": "^1.5.0",
        "@sinclair/typebox": "^0.25.24",
        "@types/jest": "^29.4.0",
        "@types/twig": "^1.12.9",
        "body-parser": "^1.20.2",
        "fastify": "^4.14.1",
        "fastify-graceful-shutdown": "^3.4.0",
        "rest-on-couch-client": "^5.3.0",
        "superagent": "^7.1.6",
        "ts-jest": "^29.0.5",
        "twig": "^1.16.0"
    },
    "scripts": {
        "build": "tsc",
        "check-types": "tsc --noEmit",
        "eslint": "eslint --cache src",
        "eslint-fix": "eslint --fix src",
        "prettier": "prettier --check src",
        "prettier-write": "prettier --write src",
        "start": "env-cmd node lib/src/server.js",
        "dev": "env-cmd nodemon --config nodemon.json src/server.ts",
        "test-only": "jest",
        "test": "npm run prettier && npm run eslint && npm run check-types && npm run test-only"
    },
    "devDependencies": {
        "@types/node": "^18.14.6",
        "@types/superagent": "^4.1.16",
        "env-cmd": "^10.1.0",
        "eslint": "^8.35.0",
        "eslint-config-zakodium": "^7.0.0",
        "fastify-tsconfig": "^1.0.1",
        "jest": "^29.5.0",
        "nodemon": "^2.0.21",
        "prettier": "^2.8.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "volta": {
        "node": "16.19.1"
    }
}
