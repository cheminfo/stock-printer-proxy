        "eslint": "^8.1.0",
        "eslint-config-cheminfo-typescript": "^10.2.0",
        "fastify-tsconfig": "^1.0.1",
        "jest": "^27.3.1",
        "nodemon": "^2.0.14",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    }
